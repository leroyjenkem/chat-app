{
  "name": "chat-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@csstools/postcss-sass": "^5.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "firebase": "^9.8.1",
    "gulp": "4",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "postcss-dark-theme-class": "^0.7.3",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "rfs": "^9.0.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build:css": "postcss src/styles/bundle/bundled.css -o src/index.css",
    "watch:css": "postcss src/styles/bundle/bundled.css -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js gulp",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "gulp": "gulp cssBundle"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
